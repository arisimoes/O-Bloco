{
  "name": "keep-notes-electron",
  "version": "0.1.0",
  "description": "Minimal Windows Electron app integrated with Google Keep",
  "main": "src/main.js",
  "scripts": {
    "start": "electron .",
    "dist": "electron-builder --win",
    "dist:win": "electron-builder --win",
    "dist:portable": "electron-builder --win portable"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "adm-zip": "^0.5.16",
    "express": "^4.18.2",
    "googleapis": "^130.0.0",
    "iconv-lite": "^0.7.2",
    "jschardet": "^3.1.4"
  },
  "devDependencies": {
    "electron": "^40.0.0",
    "electron-builder": "^24.9.0"
  },
  "build": {
    "appId": "com.example.keepnotes",
    "productName": "Keep Notes",
    "files": [
      "src/**/*",
      "package.json",
      "README.md"
    ],
    "directories": {
      "buildResources": "build"
    },
    "win": {
      "target": [
        "nsis",
        "portable"
      ]
    }
  }
}
